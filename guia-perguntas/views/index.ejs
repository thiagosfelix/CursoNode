<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia perguntas</title>
</head>
<body>
  <h1>Guia Perguntas</h1>  
  <hr>
  <h3>Lista de perguntas</h3>

  <!-- <%= %> Tag exibir valor da variÃ¡vel -->

  <p>Nome: <%= nome %></p>
  <p>Linguagem favorita: <%= lang %></p>
  <p>Empresa: <%= empresa %></p>
  <p>Inscritos: <%= inscritos %></p>

    <% if (msg){ %>
        <h3>Isso Ã© uma mensagem de erro!</h3>
    <%} else {%>
        <h3>Nenhum erro!</h3>
    <%}%>

    <% produtos.forEach(function(produto) { %>
        <hr>
        <h3><%= produto.nome %></h3>
        <h4><%= produto.preco %></h4>
    <% }) %>
</body>
</html>